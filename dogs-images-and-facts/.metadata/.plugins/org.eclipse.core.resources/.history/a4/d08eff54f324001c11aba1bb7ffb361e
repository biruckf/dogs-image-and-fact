package com.codeTest.dogsImageAndFact.model;

/**
 * @author biruckfeysa
 *
 *POJO class which will be used to create DogsFactAndImage objects 
 */
public class DogsFactAndImage {

	private String image;
	private String fact;
	
	

	public DogsFactAndImage() {
		
	}


	public DogsFactAndImage(String image, String fact) {
		this.image = image;
		this.fact = fact;
	}

	public String getImage() {
		return image;
	}

	public void setImage(String image) {
		this.image = image;
	}

	public String getFact() {
		return fact;
	}

	public void setFact(String fact) {
		this.fact = fact;
	}


	@Override
	public String toString() {
		return "DogsFactAndImage [image=" + image + ", fact=" + fact + "]";
	}

	
	
	
	
	
}
